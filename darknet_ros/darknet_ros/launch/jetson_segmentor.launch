<launch>


	<arg name="image_input" />
	<arg name="bb_pub" />
	<arg name="detectedImage_pub" />
	<arg name="machine_name" />

	<arg name="launch_prefix" default=""/>

	<arg name="yolo_weights_path"          default="$(find darknet_ros_eb)/yolo_network_config/weights"/>
	<arg name="yolo_config_path"           default="$(find darknet_ros_eb)/yolo_network_config/cfg"/>

	<arg name="network_param_file"         default="$(find darknet_ros_eb)/config/yolov3-tiny-2c.yaml"/>
	<rosparam command="load" ns="darknet_ros" file="$(arg network_param_file)"/>

	<node pkg="darknet_ros_eb" type="darknet_ros_eb" name="$(arg machine_name)" output="screen" launch-prefix="$(arg launch_prefix)" machine="$(arg machine_name)">
		<param name="weights_path"          value="$(arg yolo_weights_path)" />
		<param name="config_path"           value="$(arg yolo_config_path)" />
		<param name="image_input" 			value="$(arg image_input)"/>
		<param name="bb_pub" 			    value="$(arg bb_pub)"/>
		<param name="detectedImage_pub" 	value="$(arg detectedImage_pub)"/>
	</node>


</launch>
